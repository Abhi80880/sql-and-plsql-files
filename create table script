    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
-- Create/Recreate indexes 
create index BJAZ_HERO_WS_DATA_RECORD_DAT on BJAZ_HERO_WS_DATA (RECORD_DATE)
  tablespace BAGIC_IDX3
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
create index BJAZ_HERO_WS_RECORD_DATE_IND on BJAZ_HERO_WS_DATA (TRUNC(RECORD_DATE))
  tablespace BAGIC_IDX1
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
create unique index BJAZ_NEWHERO_POLICYNUMBER on BJAZ_HERO_WS_DATA (PROPOSALNO)
  tablespace USERS
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
-- Create/Recreate primary, unique and foreign key constraints 
alter table BJAZ_HERO_WS_DATA
  add constraint BJAZ_NEWHERO_POLNUM primary key (PROPOSALNO);
-- Grant/Revoke object privileges 
grant select, insert, update on BJAZ_HERO_WS_DATA to DB_SCRIPT;
grant select, insert, update on BJAZ_HERO_WS_DATA to DB_SCRIPTS;
grant select on BJAZ_HERO_WS_DATA to DWH_SYNC;
grant select, insert, update, delete on BJAZ_HERO_WS_DATA to EXTERNAL_GIOS_USER;
grant select, insert, update, delete on BJAZ_HERO_WS_DATA to GENERAL_OPUS_USER;
grant select on BJAZ_HERO_WS_DATA to IT_TEAM;
grant select, insert, update, delete on BJAZ_HERO_WS_DATA to IT_TEAM_DB;






